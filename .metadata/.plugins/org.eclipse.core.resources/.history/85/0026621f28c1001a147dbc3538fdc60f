package org.revature;

import org.revature.animals.Animal;
import org.revature.animals.Animal.Alive;

public class Main {

	public static void main(String[] args) {
		Animal animal = new Animal(23, "Dragon");
		
		
		Alive aliveObj = animal.getAlive();
		animal.getAlive().setBreathe(true);
		
		System.out.println(aliveObj.isAlive());
		
		
		
	}

}
