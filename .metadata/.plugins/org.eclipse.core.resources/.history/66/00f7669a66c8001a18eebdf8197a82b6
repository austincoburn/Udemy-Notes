package org.studyeasy;

public class App2 {

	public static void main(String[] args) {
		String[] names = {"Chann", "John", "Pooja", "Jim", "Josh"};
		App2 app2 = new App2();

		app2.displayNames(names);

	}
	
	void displayNames(String[] names) {
		for(String name : names) {
			System.out.println(name);
		}
	}

}
