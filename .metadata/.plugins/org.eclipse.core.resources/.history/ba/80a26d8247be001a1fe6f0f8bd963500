package org.studyeasy.shop;

public class Person {
	Student student;
	
	public Student getStudent() {
		return student;
	}



	public Person() {
		student = new Student();
	}
	
	
	
	public class Student {
		int age;
		String classs;
		
		public Student() {
			age = 9;
			classs = "Biology";
			
		}
		
		public void setAge(int age) {
			this.age = age;
		}
		
		public void setClass(String classs) {
			this.classs = classs;
		}

		public int getAge() {
			return age;
		}

		public String getClasss() {
			return classs;
		}
		
		
	
	}
}
