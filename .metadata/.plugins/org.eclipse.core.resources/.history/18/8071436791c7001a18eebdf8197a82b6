package org.studyeasy;

public class Hello {

	public static void main(String[] args) {
	
		
		Person pooja = new Person("Pooja", 26, "Female");
		System.out.println(pooja);
		pooja.setAge(-23);
		System.out.println(pooja.setAge(-23));
		
		Person austin = new Person();
		austin.setAge(23);
		austin.setGender("Male");
		austin.setName("Austin");

	}

}
