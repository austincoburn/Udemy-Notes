package org.studyeasy;

import java.util.Scanner;

public class App {

	public static void main(String[] args) {
		System.out.println("What's your age?");
		Scanner sc = new Scanner(System.in);
		String name = sc.next();
		sc.close();
		System.out.println("Hey there " + name + " hava a nice day!");
	}
	
}
