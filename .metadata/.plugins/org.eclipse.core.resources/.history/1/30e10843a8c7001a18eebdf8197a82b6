package org.studyeasy;

public class App {

	public static void main(String[] args) {
		String x = "Study";
		String y = "Easy";
		String z = x + y;

		System.out.println(z);

		if (z.equals("Studyeasy")) {
			System.out.println("Text is Studyeasy");

		} else {
			System.out.println("Text is not studyeasy");
		}
	}

}
