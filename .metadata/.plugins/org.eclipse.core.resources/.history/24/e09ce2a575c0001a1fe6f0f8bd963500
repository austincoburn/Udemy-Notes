package org.studyeasy;

import java.util.Scanner;

import org.studyeasy.shop.Door;
import org.studyeasy.shop.Person;
import org.studyeasy.shop.Person.Student;

public class Shop {
	

	public static void main(String[] args) {
		
		Person person = new Person();
		Person.Student student = person.new Student();
		System.out.println(student.getAge());
		
		Door door = new Door();
		Door.Lock locky = door.new Lock(true);
		door.getLock().setLock(false);
		door.shopStatus();
		door.getLock().isLock();
		System.out.println(locky.isLock(););

	
		
		
		
		
		
		/*
