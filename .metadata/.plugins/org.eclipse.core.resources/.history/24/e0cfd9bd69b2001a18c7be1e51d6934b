package org.studyeasy;

public class Hello {

	
	//Methods can be called numerous times and can be used unlimited times
	public static void main(String[] args) {
		
		loop();
		System.out.println("*******************");
		loop();
	
	}
	
	public static void loop(int i, int j) {
		while (i<=j) {
			System.out.println(i);
			i++;
		}
	}
	


}
