package org.studyeasy;

public class App {

	public static void main(String[] args) {
		new App().case1(3, 0);

	}

	public void case1(int x, int y) {
		if(y != 0) {
			System.out.println(x/y);
		} else {
			System.out.println("The value of y is zero");
		}
	}
	
	public void case2(int x, int y) {
		
	}
	
}
