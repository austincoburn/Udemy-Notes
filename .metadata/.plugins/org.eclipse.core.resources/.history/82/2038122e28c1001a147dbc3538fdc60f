public class Main {

	public static void main(String[] args) {
		Animal animal = new Animal(23, "Dragon");
		
		Animal.Alive alive = animal.new Alive();
		
		Alive animalObj = animal.getAlive();
		animalObj.setBreathe(true);
		
		
		
	}

}
